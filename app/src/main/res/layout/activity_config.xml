<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.savecontroladoria.activity.ConfigActivity">


    <android.support.v7.widget.RecyclerView
        android:id="@+id/rvUsuarios"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:listitem="@layout/adapter_usuarios"
        android:layout_above="@+id/btnConfok"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/btnConfok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Confirma Edição"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
